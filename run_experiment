import sys
from GeneticFunctions import *
from Problems import *
from Population import*
from PSO import *
from pandas_plink import read_plink1_bin
import pandas as pd

if len(sys.argv) != 7:
    print("Usage: python create_population.py <plink_prefix> <pheno_file> <sumstat_file> <trait_column> <config_file> <optimizer>")
    sys.exit(1)

objective_function = "BinaryClassification"  # Hardcoded as specified
prefix = sys.argv[1]
pheno_file = sys.argv[2]
sumstat_file = sys.argv[3]
trait_column = sys.argv[4]
config_file = sys.argv[5]
optimizer = sys.argv[6]

HTGSM = Population(objective_function, prefix, pheno_file, sumstat_file, trait_column, optimizer)

print("Population instance created successfully!")

HTGSM.run_experiment()
